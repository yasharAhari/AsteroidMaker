class Vertex
{
 private float _x;
 private float _y;
 private float _z;
 
 public Vertex(float x,float y,float z)
 {
  _x = x;
  _y = y;
  _z = z;
 }
  
  public float getX()
  {
   return this._x;
   
  }
  public float getY()
  {
   return this._y; 
  }
  
  public float getZ()
  {
   return this._z; 
  }
  
  
  
}
